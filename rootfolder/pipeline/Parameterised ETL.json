{
	"name": "Parameterised ETL",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "input//@{'planes.csv'}"
					},
					{
						"name": "Destination",
						"value": "dbo.@{'Planes'}"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "ICAO",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "ICAO",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "IATA",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "IATA",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "MAKER",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "MAKER",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "DESCRIPTION",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "DESCRIPTION",
									"type": "String",
									"physicalType": "nvarchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "InputCSV",
						"type": "DatasetReference",
						"parameters": {
							"fileName": "planes.csv"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "OutputTable",
						"type": "DatasetReference",
						"parameters": {
							"tableName": "Planes"
						}
					}
				]
			}
		],
		"annotations": []
	}
}